<div class="category-link-parent mgb">
    <div class="category-all">
        <%= link_to "全て", ({controller: "admin/examples", action: "index"}), class:"btn btn-info mgr" %>
    </div>
    <% categories.each do |category| %>
        <div class="category-link">
            <%= link_to category.name, ({controller: "admin/examples", action: "index", id: category.id}), class:"btn btn-info mgr" %>
        </div>
    <% end %>
</div>